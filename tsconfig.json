{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./dist/",
    "lib": [
      "DOM",
      "DOM.Iterable"
    ],
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ]
}
